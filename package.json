{
  "name": "faker-ge",
  "version": "0.0.4",
  "description": "Generate massive amounts of fake Georgian data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncfinkd/faker-ge.git"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "publish": "npm run build && npm publish",
    "watch": "npx nodemon --watch src --ext ts --exec 'npx ts-node src/index.ts'"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./types": {
      "types": "./lib/types.d.ts",
      "import": "./lib/types.js",
      "require": "./lib/types.cjs"
    }
  },
  "keywords": [
    "faker",
    "georgian",
    "faker-ge",
    "faker-georgian"
  ],
  "files": [
    "lib"
  ],
  "author": "Nika Shamiladze",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
